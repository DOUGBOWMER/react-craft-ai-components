    "babel-plugin-transform-react-jsx": "6.24.1",
    "eslint": "4.3.0",
    "eslint-config-craft-ai": "2.2.3",
    "eslint-plugin-react": "6.10.3"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "d3-hierarchy": "1.1.5",
    "d3-selection": "1.1.0",
    "d3-zoom": "1.5.0",
    "lodash": "4.17.4",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-overlays": "0.8.0"
