{
  "name": "craft-ai-react-decision-tree",
  "version": "1.0.0",
  "description": "craft ai react package to display craft ai decision tree",
  "author": {
    "name": "craft ai",
    "email": "contact@craft.ai",
    "url": "http://craft.ai/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:craft-ai/craft-ai-react-decision-tree.git"
  },
